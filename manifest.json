{
    "manifest_version": 2,
  
    "name": "Twitch Online",
    "description": "This extension will show you a list of wich of your followed channels are streaming.",
    "version": "1.0",
  
    "browser_action": {
        "default_icon": "app-icons-twitch.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [ 
        { 
            "js": [ "jquery.min.js", "twitch.js", "bootstrap.min.js", "popper.min.js" ],
            "matches": [ "http://*/*", "https://*/*"],
            "run_at": "document_end"
        }
     ], 
    "permissions": [
     
    ]
  }